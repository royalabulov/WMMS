{
    "Serilog": {
        "Using": [ "Serilog.Sinks.MSSqlServer" ],
        "MinimumLevel": "Information",
        "WriteTo": [
            {
                "Name": "MSSqlServer",
                "Args": {
                    "connectionString": "Data Source=ROYAL;Initial Catalog=WMMS;Integrated Security=SSPI;Trusted_Connection=true;TrustServerCertificate=True;",
                    "tableName": "Logs",
                    "autoCreateSqlTable": true
                }
            }
        ]
    }
}
